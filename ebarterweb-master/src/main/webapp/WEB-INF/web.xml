<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee webapp_4_0.xsd"
  version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
   <welcome-file>login.jsp</welcome-file>
</welcome-file-list>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fileimport</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.TradeFileImport</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileimport</servlet-name>
    <url-pattern>/fileimport</url-pattern>
  </servlet-mapping>

<servlet>
    <servlet-name>userreg</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.UserRegController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userreg</servlet-name>
    <url-pattern>/userreg</url-pattern>
  </servlet-mapping>
  
<servlet>
    <servlet-name>addtrade</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.AddTrade</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addtrade</servlet-name>
    <url-pattern>/addtrade</url-pattern>
  </servlet-mapping>

<servlet>
    <servlet-name>reports</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.ReportController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>reports</servlet-name>
    <url-pattern>/reports</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Calling_Registertodb_class</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.RegistrationController</servlet-class>
  </servlet>
   
<servlet-mapping>  
<servlet-name>Calling_Registertodb_class</servlet-name>  
<url-pattern>/htmltodb</url-pattern>  
</servlet-mapping> 




























</web-app>